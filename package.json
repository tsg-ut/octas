{
  "name": "octas",
  "version": "1.0.0",
  "description": "一筆書きゲーム",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify lib/index.js --debug --transform [ babelify --presets [ es2015 ] ] | exorcist index.js.map > index.js",
    "pug": "pug index.pug",
    "less": "lessc index.less | postcss --use autoprefixer -o index.css",
    "build": "concurrently \"npm run browserify\" \"npm run pug\" \"npm run less\"",
    "lint": "eslint \"lib/**/*.js\"",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurgm/octas.git"
  },
  "author": "kurgm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurgm/octas/issues"
  },
  "homepage": "https://github.com/kurgm/octas#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.18.0",
    "exorcist": "^0.4.0",
    "jquery": "^3.1.0",
    "less": "^2.7.1",
    "postcss-cli": "^3.2.0",
    "pug-cli": "^1.0.0-alpha6",
    "snapsvg": "^0.5.1"
  }
}
